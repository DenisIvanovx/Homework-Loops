using System;

    class Program
    {
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());
            Random rnd = new Random();

            for (int i = 0; i < n; i++)
            {
                int randomNumber = rnd.Next(1, n + 1);
                Console.WriteLine(randomNumber);
            }
        }
    }
